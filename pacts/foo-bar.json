{
  "consumer": {
    "name": "Foo"
  },
  "provider": {
    "name": "Bar"
  },
  "interactions": [
    {
      "description": "a CORS request with matchers for Access-Control-Allow-Origin",
      "request": {
        "method": "GET",
        "path": "/foo",
        "headers": {
          "Origin": "http://myservice.com"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Access-Control-Allow-Origin": {
            "json_class": "Pact::SomethingLike",
            "contents": "*"
          }
        },
        "body": "Hello world"
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "1.0.0"
    }
  }
}